{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "es5",
    "removeComments": true,
    "rootDir": "./",
    "outDir": "./out",
    "strict": true,
    "strictNullChecks": true,
    "useUnknownInCatchVariables": false,
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "downlevelIteration": true
  },
  "exclude": [
    "**/*.spec.ts"
  ]
}
